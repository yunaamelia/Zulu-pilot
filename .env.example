# Google Cloud / Gemini API Configuration
# Copy this file to .env and fill in your values

# For Gemini 3.0 Pro Preview (using @google/genai SDK with Vertex AI)
# Get your API key from: https://aistudio.google.com/apikey
GOOGLE_CLOUD_API_KEY=your-api-key-here

# Vertex AI Configuration (required for Gemini 3.0 Pro Preview)
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_CLOUD_LOCATION=global
# Note: Gemini 3.0 Pro Preview uses 'global' location, not region-specific

# Alternative: Use GOOGLE_API_KEY or GEMINI_API_KEY (auto-detected by @google/genai)
# GOOGLE_API_KEY=your-api-key-here
# GEMINI_API_KEY=your-api-key-here

# For Vertex AI (if using Vertex AI instead of Gemini API)
# GOOGLE_GENAI_USE_VERTEXAI=true

# Debug mode
# DEBUG=false
# ZULU_PILOT_DEBUG=false
